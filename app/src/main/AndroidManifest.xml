<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="io.innofang.wintertraining">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.DialogActivity">
        </activity>
        <activity android:name=".activity.LearnForActivity">
        </activity>
        <activity android:name=".activity.LuckyActivity">
        </activity>
        <activity android:name=".activity.LearnFragmentActivity">
        </activity>
        <activity android:name=".activity.ViewPagerActivity">
        </activity>
        <activity android:name=".activity.PermissionsActivity">
        </activity>
        <activity android:name=".activity.MenuActivity">
        </activity>
        <activity android:name=".activity.RecyclerViewActivity">
        </activity>
    </application>

</manifest>